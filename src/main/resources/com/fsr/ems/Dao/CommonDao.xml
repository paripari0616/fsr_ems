<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fsr.ems.Dao.CommonDao">
    <select id="getMasterInfo" resultType="com.fsr.ems.dto.SelectOption">
        SELECT 
        	code,
        	name
        FROM m_code WHERE type = #{masterCode}
    </select>
</mapper>