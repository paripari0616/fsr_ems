<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fsr.ems.Dao.CommonDao">
    <select id="getParty_b" resultType="com.fsr.ems.dto.Party_B">
        SELECT
        company_id,
        address,
        tel,
        fax
        FROM party_b WHERE type = #{masterCode}
    </select>
</mapper>